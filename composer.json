{
    "name": "codicastudio/validationrules",
    "description": "A random Codica Studio package.",
    "keywords": [
        ""
    ],
    "homepage": "https://github.com/codicastudio/validationrules",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "league/iso3166": "^2.1.5"
    },
    "require-dev": {
        "myclabs/php-enum": "^1.6",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "codicastudio/enum": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ValidationRules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ValidationRules\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=no"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ValidationRules\\ValidationRulesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
